{
  "name": "@open-ayame/ayame-web-sdk",
  "version": "19.09.0",
  "description": "Web SDK for WebRTC Signaling Server Ayame",
  "main": "dist/ayame.min.js",
  "scripts": {
    "watch": "rollup -w -c ./rollup.config.js",
    "build": "rollup -c ./rollup.config.js",
    "lint": "$(npm bin)/eslint --ext .tsx,.ts src/",
    "fmt": "yarn lint --fix",
    "deploy:docs": "$(npm bin)/gh-pages -d docs",
    "esdoc": "$(npm bin)/esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAyame/ayame-web-sdk.git"
  },
  "directories": {
    "doc": "docs/"
  },
  "homepage": "https://github.com/OpenAyame/ayame-web-sdk",
  "author": "Shiguredo Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^12.7.8",
    "@types/webrtc": "^0.0.25",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2",
    "rollup": "^1.22.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "typescript": "^3.6.3"
  }
